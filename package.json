{
  "name": "nairaboom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "dev2": "cross-env NODE_OPTIONS='--inspect' next dev",
    "cloud-deploy": "yarn build && zip -r out.zip out .htaccess && gcloud compute scp --project='nairaboom-be' --zone='europe-west1-b' out.zip nairaboom-php-server:/var/www/nairaboom.ng/public_html && rm -rf out.zip",
    "cloud-deploy-staging": "yarn build && zip -r out.zip out .htaccess && gcloud compute scp --project='nairaboom-be' --zone='europe-west1-b' out.zip nairaboom-php-server:/var/www/nb.nairaboom.ng/public_html && rm -rf out.zip",
    "deploy": "gcloud compute ssh --zone 'europe-west1-b' root@nairaboom-php-server --project 'nairaboom-be' && sudo ./deploy-frontend.sh out.zip"
  },
  "dependencies": {
